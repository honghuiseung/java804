package sec02.exam02;

public class exam01 {

	public static void main(String[] args) {
		int max =0;
		int[] array = {1,5,4,8,2};
		
		for (int i = 0; i < array.length; i++) {
			for (int j = i; j < array.length; j++) {
				if(array[i]>array[j]) {
					int num;
					num = array[i];
					array[j] = array[i];
				}
			}
		}
		
		System.out.println(array[4] + max);
	}

}
