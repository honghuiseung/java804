package sec06.exam06;

public class Car {
	//필드
	private int speed;
	private int stop;
	
	//생성자
	
	//메소드
	//source>>Generate Getters and Setters>>메서드 생성
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	public int getStop() {
		return stop;
	}
	public void setStop(int stop) {
		this.stop = stop;
	}
	
}
